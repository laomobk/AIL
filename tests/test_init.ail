
struct Student {
    __name
    __score
}

fun (Student) __init__(self, name, score) {
    self.__name = name
    self.__score = score
}

fun (Student) getName(self) {
    return self.__name
}

fun (Student) getScore(self) {
    return self.__score
}

stu = Student('Laomo', 150)
print stu.getName(); stu.getScore()

