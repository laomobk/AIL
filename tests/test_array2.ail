
cases = []

fun testNormalArrayAccess() {
    a = ['a', 'b', 'c', 'Nezha']
    print 'a:'; a

    for (i = -10; i < 10; i = i + 1) {
        print 'access index'; i
        try {
            print 'a[i] ='; a[i]
        } catch err {
            print err.err_msg
        }
    }
}
// cases.append(testNormalArrayAccess)

fun testTwoDimArrayAccess() {
    a = [[0, 0, 1, 0, 0],
         [0, 1, 0, 1, 0],
         [1, 0, 2, 0, 1]]

    print 'a ='; a
    
    print 'access line'
    y = -10
    while y < len(a) {
        try {
            print ' '.join(a[y])
        } catch err {
            print err.err_msg
        }
        y = y + 1
    }
}
cases.append(testTwoDimArrayAccess)


if __is_main__ {
    for (i = 0; i < len(cases); ++i) {
        print '\ncall:'; cases[i]
        cases[i]()
    }
}

