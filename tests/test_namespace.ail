begin


name = 'Nezha'


fun set_name_no_global() is
    name = 'Zhaer'
end


fun set_name_with_global() is
    global.name = 'Zhaer'
end


fun set_name_with_local() is
    local.name = 'Nezha'
end


print 'before set name, name =' ; name

set_name_no_global()
print 'after set_name_no_global(), name =' ; name

set_name_with_global()
print 'after set_name_with_global(), name =' ; name

set_name_with_local()
print 'after set_name_with_local(), name =' ; name

end
