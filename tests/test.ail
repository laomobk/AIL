

class case {
    init(self, case_func) {
        self.__func = case_func
    }

    func __match__(self, target) {
        return self.__func(target)
    }
}


class Point {
    init(self, x: Integer, y: Integer) {
        self.x = x;
        self.y = y;
    }

    func __match__(self, target): Boolean {
        return self.x == target.x and self.y == target.y
    }
}


print match Point(3, 4) {
    Point!{ x: 3, y: 4 }: 'yes',
    else: 'undefined',
}

