

fun log(func) {
    fun wrapper(*argv) {
        print 'call'; func
        return func(*argv)
    }
    return wrapper
}

fun levelLog(level) {
    fun wrapper(func) {
        fun inner(*argv) {
            print 'level:'; level
            return func(*argv)
        }
        return inner
    }
    return wrapper
}

@levelLog('info')
fun printState() {
    u
    print 'State: good'
}


if __is_main__ {
    printState()
}
