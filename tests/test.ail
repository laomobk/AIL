class FuncArg {

    func __str__(self): String {
        return '({}{}{})'.format(
            ', '.join(self.args),
            match self.vararg {
                null: '',
                else: ', *{}'.format(self.vararg),
            },
            match self.kwarg {
                null: '',
                else: ', **{}'.format(self.kwarg),
            }
        )
    }
}

