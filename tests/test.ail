begin

fun guess_it(number) is
	if number == 726 then
		return 0
	else
	    if number < 726 then
		    return -1
	    else
	        if number > 726 then
		        return 1
		    endif
	    endif
	endif
end

print "Let's guess this number (300 ~ 1000)"
print "You have 5 lives!"

lives = 5

do
	input "Guess :" ; num

	s = guess_it(num)

	if s == -1 then
		print "too small!"
	endif

	if s == 1 then
		print "too large!!!"
	endif

	if (lives = lives - 1) == 0 then
		print "all lives run out!  game over!"
	endif
loop until s == 0

end
